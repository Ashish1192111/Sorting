#include<stdio.h>
void main()
{
    int a[50],size,num,i;
    printf("Enter the size of the array : \n");
    scanf("%d",&size);
    printf("Enter the elements one by one : \n");
    for(i=0;i<size;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Elements are : \n");
    for(i=0;i<size;i++)
    {
        printf("%d\t",a[i]);
    }
    if(size>=50)
    {
        printf("Overflow Condition ");
    }
    else
    {
        printf("\nEnter the number which you want to insert : \n");
        scanf("%d",&num);
        a[size] = num;
        size++;
        printf("Elements are : \n");
        for(i=0;i<size;i++)
        {
            printf("%d\t",a[i]);
        }
    }

}
