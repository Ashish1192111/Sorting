#include<stdio.h>
void main()
{
    int a[20],size,i;
    printf("Enter the Size of Array : \n  ");
    scanf("%d",&size);
    printf("Enter the elements of array :\n ");
    for(i=0;i<size;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Elements are : \n");
    for(i=0;i<size;i++)
    {
        printf("%d\t",a[i]);
    }
}

